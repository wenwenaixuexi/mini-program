 text-align: center;
  }
.minebu{
  /* width: 414rpx;
  height: 667rpx;  */
  background-color: white;
  height: 100%;
  box-sizing: border-box;
  font-family: "幼圆";
  font-family: 'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif;

}
.sub_nav {
  width: 100%;
  height: 180rpx;
  display: flex;
  border-radius:90rpx;
}

.sub_nav .nav_item {
  background: white;
  width: 25%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-left:50px;
  margin-top: 5px;
  
}

.sub_nav .nav_item image {
  background: white;
  width: 120rpx;
  height: 120rpx;
  border-radius: 60rpx;

}

.sub_nav .nav_item text {
  background: white;
  font-size: 30rpx;
}
